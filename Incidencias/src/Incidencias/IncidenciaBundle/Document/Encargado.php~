/**
 * Description of Encargado
 *
 * @author tuguu
 */

namespace Incidencias\IncidenciaBundle\Document;
use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Encargado {
    
    /**
     * @MongoDB\Id
     */
    protected $id;
    /**
     * @MongoDB\ReferenceOne(targetDocument="Empleado")
     */
    protected $empleado;
    /**
     * @MongoDB\String
     */
    protected $user;
    /**
     * @MongoDB\String
     */
    protected $password;
}

?>
